{
  "name": "input-search",
  "version": "0.1.0",
  "description": "This is a tested jQuery HTML5 polyfill for <code>input[type=\"search\"]</code>.\nIts goal is to provide a cross-browser <em>fully-featured <code>input</code> element</em> without extra-markup.",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "grunt test",
    "postinstall": "bower install && grunt build-quick"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/oncletom/input-search.git"
  },
  "keywords": [
    "jquery",
    "plugin",
    "input",
    "search",
    "html5",
    "polyfill"
  ],
  "author": "Thomas Parisot <thomas@oncle-tom.net>",
  "license": "MIT",
  "readmeFilename": "README.md",
  "devDependencies": {
    "grunt-jquerymanifest": "~0.1.0",
    "grunt": "~0.4.1",
    "grunt-contrib-qunit": "~0.2.0",
    "grunt-contrib-copy": "~0.4.0"
  },
  "dependencies": {
    "bower": "~1.7.5"
  }
}
